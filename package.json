{
  "name": "webapp-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "dev": "nodemon ./src/bin/www --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "express-ejs-layouts": "^2.3.1",
    "express-session": "^1.15.6",
    "jwt-simple": "^0.5.1",
    "marked": "^0.3.6",
    "moment": "^2.20.1",
    "mongoose": "^4.13.2",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "dotenv": "^4.0.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  }
}
